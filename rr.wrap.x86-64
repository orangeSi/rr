
#/bin/sh
source /home/sikaiwei/crystal/env.sh
base=`dirname $(readlink -f $0)`
bin_name=rr
binary=/home/sikaiwei/rr

if [ ! -f "$binary" ]
then

	if [ ! -f "$base/$bin_name" ];
	then
		echo "error, $binary or $base/$bin_name both not exists"
		exit
	else
		binary=$base/$bin_name
	fi
fi


if [ "$1" == "" ];
then
	#$binary --help
	$binary
	exit
fi
$binary $@ &&
echo done

